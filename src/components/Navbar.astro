<script>
    let navlinks = document.querySelectorAll('.nav-links a');
    navlinks.forEach(nav => {
        nav.addEventListener("click",()=>{
            /* alert('hiciste click') */
            navlinks.forEach(a => {
                a.classList.remove('active')
            })
            nav.classList.add('active');
        })
    })
</script>
